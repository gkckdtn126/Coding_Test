    "numbers = [0, 0, 0, 0]\n",
    "def solution(numbers):\n",
    "    answer = ''\n",
    "    numbers_str = [str(num) for num in numbers]\n",
    "    numbers_str.sort(key=lambda num: num*3, reverse=True)\n",
    "    #3자리 수이므로 3번 반복한다. \n",
    "    answer = ''.join(numbers_str)\n",
    "    return  f\"{int(answer)}\"\n",
    "solution(numbers)"
   ]
